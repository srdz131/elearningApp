<h2>{{instructor.username}}'s Classes</h2>
<table width="50%">
    <thead>
    <tr>
        <th width="200">Class ID</th>

        <th width="150">Class Name</th>
          <th></th>
            <th></th>
    </tr>
    </thead>
    <tbody>
      {{#each instructor.classes}}
        <tr>
          <td>{{class_id}}</td>
          <td>{{class_title}}</td>
          <td><a href="/classes/{{class_id}}/lessons">View Lessons</a></td>
          <td><a href="/instructors/classes/{{class_id}}/lessons/new">Add Lessons</a></td>
        </tr>
        {{/each}}
    </tbody>
  </table>
